{
  "name": "aria2-extension",
  "version": "0.3.5",
  "private": true,
  "dependencies": {
    "@emotion/react": "11.7.0",
    "@emotion/styled": "11.6.0",
    "@fontsource/roboto": "4.5.1",
    "@mui/icons-material": "5.2.0",
    "@mui/material": "5.2.2",
    "aria2": "4.1.2",
    "codemirror": "5.64.0",
    "react": "17.0.2",
    "react-codemirror2": "7.2.1",
    "react-dom": "17.0.2",
    "react-router-dom": "6.0.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "3.1.1",
    "@types/node": "14.17.34",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/webextension-polyfill": "0.8.2",
    "@types/webpack": "4.41.32",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "css-loader": "5.2.7",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.1",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-standard": "4.1.0",
    "husky": "7.0.4",
    "lint-staged": "12.1.2",
    "prettier": "2.5.0",
    "react-scripts": "4.0.3",
    "style-loader": "2.0.0",
    "ts-loader": "8.3.0",
    "typescript": "4.5.2",
    "web-ext": "6.5.0",
    "webextension-polyfill": "0.8.0",
    "webpack-cli": "4.9.1"
  },
  "scripts": {
    "dev": "npm run build && web-ext run --source-dir ./build/",
    "build_scripts": "webpack --config webpack.config.js",
    "build": "INLINE_RUNTIME_CHUNK=false react-scripts build && npm run build_scripts && cp -rf AriaNg build",
    "package": "web-ext build --config=webext.config.js",
    "complete": "npm run build && npm run package",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.9.0 <8.0.0"
  },
  "engine-strict": true,
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "npm run lint",
      "npm run format"
    ],
    "*.{js,css,md}": "prettier --write"
  }
}
