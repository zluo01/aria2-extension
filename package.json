{
  "name": "aria2-extension",
  "displayName": "Aria2 Integration Extension",
  "version": "0.5.6",
  "private": true,
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@mui/icons-material": "6.5.0",
    "@mui/material": "6.5.0",
    "@tanstack/react-form": "1.23.4",
    "@tanstack/react-query": "5.90.2",
    "@tanstack/react-router": "1.132.7",
    "aria2": "4.1.2",
    "lru-cache": "11.2.2",
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "9.36.0",
    "@jest/globals": "30.1.2",
    "@tanstack/eslint-plugin-query": "5.90.2",
    "@tanstack/router-plugin": "1.132.7",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/node": "22.18.6",
    "@types/react": "19.1.14",
    "@types/react-dom": "19.1.9",
    "@types/webextension-polyfill": "0.12.3",
    "@vitejs/plugin-react-swc": "4.1.0",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "jest": "30.1.3",
    "lint-staged": "16.2.1",
    "prettier": "3.6.2",
    "ts-jest": "29.4.4",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.1",
    "vite": "7.1.7",
    "vite-plugin-eslint": "1.8.1",
    "vite-tsconfig-paths": "5.1.4",
    "web-ext": "8.10.0",
    "webextension-polyfill": "0.12.0"
  },
  "scripts": {
    "dev": "npm run build && web-ext run --source-dir ./build/",
    "build": "vite build",
    "package": "web-ext build --config=webext.config.cjs",
    "complete": "npm run build && npm run package",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepare": "husky",
    "test": "jest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --cache --fix",
      "npm run lint",
      "npm run format"
    ],
    "*.{js,css,md}": "prettier --write"
  }
}
