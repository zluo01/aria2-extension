{
  "name": "aria2-extension",
  "displayName": "Aria2 Integration Extension",
  "version": "0.6.0",
  "private": true,
  "dependencies": {
    "@radix-ui/react-checkbox": "1.3.3",
    "@radix-ui/react-label": "2.1.8",
    "@radix-ui/react-progress": "1.1.8",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-separator": "1.1.8",
    "@radix-ui/react-slot": "1.2.4",
    "@tailwindcss/vite": "4.1.17",
    "@tanstack/react-form": "1.27.1",
    "@tanstack/react-query": "5.90.12",
    "@tanstack/react-router": "1.140.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lru-cache": "11.2.4",
    "lucide-react": "0.556.0",
    "next-themes": "0.4.6",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "4.1.17"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@jest/globals": "30.2.0",
    "@tanstack/eslint-plugin-query": "5.91.2",
    "@tanstack/eslint-plugin-router": "1.140.0",
    "@tanstack/router-plugin": "1.140.0",
    "@trivago/prettier-plugin-sort-imports": "6.0.0",
    "@types/node": "22.19.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@types/webextension-polyfill": "0.12.4",
    "@vitejs/plugin-react": "5.1.2",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "lint-staged": "16.2.7",
    "prettier": "3.7.4",
    "ts-jest": "29.4.6",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.48.1",
    "vite": "7.2.6",
    "vite-tsconfig-paths": "5.1.4",
    "web-ext": "9.2.0",
    "webextension-polyfill": "0.12.0"
  },
  "scripts": {
    "analyze": "npx vite-bundle-visualizer",
    "dev": "vite build --mode dev && web-ext run --source-dir ./build/",
    "build": "vite build",
    "package": "web-ext build --config=webext.config.cjs",
    "complete": "pnpm run build && pnpm run package",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepare": "husky",
    "test": "jest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --cache --fix",
      "pnpm run lint",
      "pnpm run format"
    ],
    "*.{js,css,md}": "prettier --write"
  }
}
